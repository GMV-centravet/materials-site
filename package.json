{
  "name": "@gmvdev/materials-site",
  "version": "0.0.1",
  "description": "materials-site",
  "license": "MIT",
  "module": "dist/index.mjs",
  "main": "dist/index.js",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "npm run generate:apis && stencil build && npm run netlify:prepare",
    "download-readme-files": "node scripts/download-docs-from-github && cd docs/dist/src && rsync -R components/**/readme.md ../.. && cd ../.. && rm -rf dist",
    "clean": "rm -rf _docs && rm -rf docs/components/**",
    "generate:apis": "npm run clean && npm run download-readme-files && markdown-folder-to-html && node scripts/generate-api-pages",
    "start": "npm run generate:apis && stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watch",
    "netlify:prepare": "cp netlify/* www/"
  },
  "dependencies": {
    "@material/animation": "~1.0.0",
    "@material/base": "~1.0.0",
    "@material/button": "~2.1.1",
    "@material/card": "~2.1.1",
    "@material/checkbox": "~2.1.1",
    "@material/chips": "~2.1.1",
    "@material/dialog": "^3.2.0",
    "@material/drawer": "^3.2.0",
    "@material/elevation": "^3.1.0",
    "@material/fab": "^3.2.0",
    "@material/floating-label": "^3.2.0",
    "@material/form-field": "^3.2.0",
    "@material/icon-button": "^3.2.0",
    "@material/layout-grid": "^3.1.0",
    "@material/line-ripple": "^3.1.0",
    "@material/linear-progress": "^3.2.0",
    "@material/list": "^3.2.0",
    "@material/menu": "^3.2.0",
    "@material/notched-outline": "^3.2.0",
    "@material/radio": "^3.2.0",
    "@material/ripple": "^3.2.0",
    "@material/select": "^3.2.0",
    "@material/snackbar": "^3.2.0",
    "@material/switch": "^3.2.0",
    "@material/tab-bar": "^3.2.0",
    "@material/textfield": "^3.2.0",
    "@material/theme": "^3.1.0",
    "@material/top-app-bar": "^3.2.0",
    "@material/typography": "^3.1.0",
    "@vaadin/router": "^1.2.0",
    "material-design-icons": "^3.0.1"
  },
  "devDependencies": {
    "@gmvdev/materials": "^1.4.1-b944c5af2fd48f3b15cbcdebd05984a2941c8647.0",
    "@stencil/core": "^1.2.4",
    "@stencil/sass": "^1.0.1",
    "@types/jest": "24.0.13",
    "@types/puppeteer": "1.12.2",
    "del-cli": "^2.0.0",
    "download-git-repo": "^2.0.0",
    "jest": "24.8.0",
    "jest-cli": "24.8.0",
    "markdown-folder-to-html": "^2.2.0",
    "puppeteer": "1.17.0",
    "readdirp": "^3.0.2",
    "workbox-build": "4.3.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GMV-centravet/materials-site.git"
  },
  "bugs": {
